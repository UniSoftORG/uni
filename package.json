{
  "name": "uni-syntax-highlighter",
  "displayName": "UNI Syntax Highlighter",
  "description": "Syntax highlighting for .uni files",
  "version": "1.0.0",
  "main": "extension.js",
  "icon": "images/unisoft.png",
  "publisher": "unisoft",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:uni.parseFile"
  ],
  "contributes": {
    "languages": [{
      "id": "uni",
      "aliases": ["UNI", "uni"],
      "extensions": [".uni"]
    }],
    "grammars": [{
      "language": "uni",
      "scopeName": "source.uni",
      "path": "./syntaxes/uni.tmLanguage.json"
    }],
    "commands": [
      {
        "command": "uni.parseFile",
        "title": "Parse UNI File"
      }
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "vscode": "^1.1.37",
    "eslint": "^7.32.0",
    "typescript": "^4.5.2"
  },
  "author": "Your Name",
  "license": "MIT"
}
